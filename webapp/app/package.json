{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "18.20.8",
        "npm": "10.9.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint:prettier": "prettier --check .",
        "fix:prettier": "prettier --write .",
        "lint:eslint": "eslint --ext \".js,.ts\" --ignore-path .gitignore .",
        "fix:eslint": "eslint --fix --ext \".js,.ts\" --ignore-path .gitignore .",
        "lint": "npm run lint:prettier && npm run lint:eslint",
        "fix": "npm run fix:prettier && npm run fix:eslint"
    },
    "dependencies": {
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@fontsource/roboto": "^5.0.13",
        "@mui/icons-material": "^5.16.5",
        "@mui/material": "^5.16.5",
        "@mui/styled-engine-sc": "^6.0.0-alpha.18",
        "@testing-library/jest-dom": "5.17.0",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.6.1",
        "cdate": "^0.0.7",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-router-dom": "6.30.1",
        "react-scripts": "^5.0.1",
        "styled-components": "^6.1.12",
        "use-http": "1.0.28",
        "web-vitals": "2.1.4"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "eslint": "^8.46.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.33.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.10",
        "prettier": "^3.0.1"
    },
    "lint-staged": {
        "**/*.{js,css,scss,html,md,yml,yaml},(!src)/**/*.ts": "prettier --write",
        "src/**/*.{js,css,scss,html,md,yml,yaml}": [
            "eslint --fix --ext \".js,.ts\" --ignore-path .gitignore .",
            "prettier --write"
        ]
    }
}
